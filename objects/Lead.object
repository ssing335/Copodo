    <validationRules>
        <fullName>Contacted_with_Customers</fullName>
        <active>true</active>
        <description>Contacted_with_Customers</description>
        <errorConditionFormula>IF(ISPICKVAL(Status, &apos;Contacted&apos;) &amp;&amp; Contacted_with_Customer__c =false,  true, false)</errorConditionFormula>
        <errorMessage>&quot;Please check the Contacted with Customer field&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Do_not_Convert_Unqualified_lead</fullName>
        <active>true</active>
        <description>Do_not_Convert_Unqualified_lead</description>
        <errorConditionFormula>IF(  ISPICKVAL(Status, &apos;Qualified&apos;) &amp;&amp;  Unqualified_Lead__c = true  , true, false)</errorConditionFormula>
        <errorMessage>&quot;This lead is not qualified to be converted&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Promotional_Email</fullName>
        <active>true</active>
        <description>Promotional Email before converting to Nurturing status</description>
        <errorConditionFormula>IF( ISPICKVAL( Status , &apos;Nurturing&apos;) &amp;&amp;  Promotion_Email__c = false , true, false)</errorConditionFormula>
        <errorMessage>&quot;Promotional Email field should be checked&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unqualified_Read_only</fullName>
        <active>false</active>
        <description>to make page layout not editable.</description>
        <errorConditionFormula>IF(ISPICKVAL(Status,&apos;Unqualified&apos;)||
ISPICKVAL(Status,&apos;UnQ Converted&apos;)||
ISCHANGED(Status) ||
ISCHANGED(Address) ||
ISCHANGED(Company) ||
ISCHANGED(Status) ||
ISCHANGED(NumberOfEmployees) ||
ISCHANGED(AnnualRevenue) ||
ISCHANGED(Industry) ||
ISCHANGED(LeadSource) ||
ISCHANGED(Contacted_with_Customer__c) ||
ISCHANGED(Phone) ||
ISCHANGED(Promotion_Email__c) ||
ISCHANGED(Unqualified_Lead__c) ||
ISCHANGED(Email) ||
ISCHANGED(Rating) ||
ISCHANGED(Title) ||
ISCHANGED(Website), true , false)</errorConditionFormula>
        <errorMessage>You Can not edit this page.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Unqualified_Rule</fullName>
        <active>true</active>
        <description>This validation rule is used to identify whether lead is unqualified or not</description>
        <errorConditionFormula>IF( ISPICKVAL( Status , &apos;Unqualified&apos;) &amp;&amp;  Unqualified_Lead__c = false , true, false)</errorConditionFormula>
        <errorMessage>&apos;&apos;Please check Unqualified Lead field&quot;</errorMessage>
    </validationRules>
